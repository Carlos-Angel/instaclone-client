ner cache\n      const { getUser } = cache.readQuery({\n        query: GET_USER,\n        variables: {\n          username: auth.username,\n        },\n      });\n      // reescribir cache\n      cache.writeQuery({\n        query: GET_USER,\n        variables: {\n          username: auth.username,\n        },\n        data: {\n          getUser: {\n            ...getUser,\n            avatar: updateAvatar.urlAvatar,\n          },\n        },\n      });\n    },\n  });\n\n  const [deleteAvatar] = useMutation(DELETE_AVATAR, {\n    update(cache) {\n      const { getUser } = cache.readQuery({\n        query: GET_USER,\n        variables: {\n          username: auth.username,\n        },\n      });\n\n      cache.writeQuery({\n        query: GET_USER,\n        variables: { username: auth.username },\n        data: {\n          getUser: {\n            ...getUser,\n            avatar: \"\",\n          },\n        },\n      });\n    },\n  });\n\n  const onDrop = useCallback(async (acceptedFile) => {\n    const file = acceptedFile[0];\n    try {\n      setLoading(true);\n      const { data } = await updateAvatar({ variables: { file } });\n      if (!data.updateAvatar.status) {\n        toast.warning(\"Error al actualizar el avatar\");\n      } else {\n        setShowModal(false);\n      }\n      setLoading(false);\n    } catch (error) {\n      console.log(error);\n    }\n  }, []);\n\n  const { getRootProps, getInputProps } = useDropzone({\n    accept: \"image/jpeg, image/jpg, image/png\",\n    noKeyboard: true,\n    multiple: false,\n    onDrop,\n  });\n\n  const onDeleteAvatar = async () => {\n    try {\n      const { data } = await deleteAvatar();\n      if (!data.deleteAvatar) {\n        toast.warning(\"error al borrar el avatar\");\n      } else {\n        setShowModal(false);\n      }\n    } catch (error) {\n      toast.warning(\"error al borrar el avatar\");\n    }\n  };\n\n  return (\n    <div className=\"avatar-form\">\n      <Button {...getRootProps()} loading={loading}>\n        Subir Foto de perfil\n      </Button>\n      <Button onClick={onDeleteAvatar}>Eliminar Foto perfil</Button>\n      <Button onClick={() => setShowModal(false)}>Cancelar</Button>\n      <input {...getInputProps()} />\n    </div>\n  );\n}\n","/home/charly/Documentos/proyectos/instaclone-client/src/components/User/Profile/Profile.js",["171"],"/home/charly/Documentos/proyectos/instaclone-client/src/components/User/Profile/index.js",[],"/home/charly/Documentos/proyectos/instaclone-client/src/components/Modal/ModalBasic/ModalBasic.js",[],"/home/charly/Documentos/proyectos/instaclone-client/src/components/User/AvatarForm/index.js",[],"/home/charly/Documentos/proyectos/instaclone-client/src/components/User/Profile/HeaderProfile/index.js",[],"/home/charly/Documentos/proyectos/instaclone-client/src/components/User/SettingsForm/SettingsForm.js",[],"/home/charly/Documentos/proyectos/instaclone-client/src/components/User/PasswordForm/PasswordForm.js",[],{"ruleId":"172","replacedBy":"173"},{"ruleId":"174","replacedBy":"175"},{"ruleId":"172","replacedBy":"176"},{"ruleId":"174","replacedBy":"177"},{"ruleId":"172","replacedBy":"178"},{"ruleId":"174","replacedBy":"179"},{"ruleId":"172","replacedBy":"180"},{"ruleId":"174","replacedBy":"181"},{"ruleId":"172","replacedBy":"182"},{"ruleId":"174","replacedBy":"183"},{"ruleId":"172","replacedBy":"184"},{"ruleId":"174","replacedBy":"185"},{"ruleId":"186","severity":1,"message":"187","line":4,"column":1,"nodeType":"188","endLine":4,"endColumn":46},{"ruleId":"172","replacedBy":"189"},{"ruleId":"174","replacedBy":"190"},{"ruleId":"191","severity":1,"message":"192","line":72,"column":6,"nodeType":"193","endLine":72,"endColumn":8,"suggestions":"194"},{"ruleId":"195","severity":1,"message":"196","line":76,"column":61,"nodeType":"197","endLine":76,"endColumn":76},"no-native-reassign",["198"],"no-negated-in-lhs",["199"],["198"],["199"],["198"],["199"],["198"],["199"],["198"],["199"],["198"],["199"],"import/no-anonymous-default-export",