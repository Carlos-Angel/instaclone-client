\n        const { data } = await updateUser({\n          variables: {\n            input: dataForm,\n          },\n        });\n        if (!data.updateUser) {\n          toast.error(\"Error al actualizar descripción\");\n        } else {\n          toast.success(\"Descripción actualizada\");\n          refetch();\n          setShowModal(false);\n        }\n      } catch (error) {\n        toast.error(\"Error al actualizar descripción\");\n      }\n    },\n  });\n\n  return (\n    <Form className=\"description-form\" onSubmit={formik.handleSubmit}>\n      <TextArea\n        name=\"description\"\n        value={formik.values.description}\n        onChange={formik.handleChange}\n        className={formik.errors.description && \"error\"}\n      />\n      <Button type=\"submit\" className=\"btn-submit\">\n        Actualizar\n      </Button>\n    </Form>\n  );\n}\n","/home/charly/Documentos/proyectos/instaclone-client/src/components/User/SettingsForm/index.js",[],"/home/charly/Documentos/proyectos/instaclone-client/src/gql/follow.js",[],"/home/charly/Documentos/proyectos/instaclone-client/src/components/User/Profile/Followers/index.js",[],"/home/charly/Documentos/proyectos/instaclone-client/src/components/User/Profile/Followers/Followers.js",[],"/home/charly/Documentos/proyectos/instaclone-client/src/components/Header/Search/index.js",[],"/home/charly/Documentos/proyectos/instaclone-client/src/components/User/DescriptionForm/index.js",[],"/home/charly/Documentos/proyectos/instaclone-client/src/components/User/ListUsers/ListUser.js",[],"/home/charly/Documentos/proyectos/instaclone-client/src/components/User/ListUsers/index.js",[],"/home/charly/Documentos/proyectos/instaclone-client/src/components/Modal/ModalUpload/ModalUpload.js",["307"],"import React, { useState, useCallback } from \"react\";\nimport { useMutation } from \"@apollo/client\";\nimport { PUBLISH } from \"../.../../../../gql/publication\";\nimport { Modal, Icon, Button, Dimmer, Loader } from \"semantic-ui-react\";\nimport { toast } from \"react-toastify\";\nimport { useDropzone } from \"react-dropzone\";\nimport \"./ModalUpload.scss\";\n\nexport default function ModalUpload({ show, setShow }) {\n  const [fileUpload, setFileUpload] = useState(null);\n  const [isLoading, setIsLoading] = useState(false);\n  const [publish] = useMutation(PUBLISH);\n\n  const onDrop = useCallback((acceptedFile) => {\n    const file = acceptedFile[0];\n    setFileUpload({\n      type: \"image\",\n      file,\n      preview: URL.createObjectURL(file),\n    });\n    console.log(file);\n  });\n\n  const { getRootProps, getInputProps } = useDropzone({\n    accept: \"image/jpeg, image/jpg, image/png\",\n    noKeyboard: true,\n    multiple: false,\n    onDrop,\n  });\n\n  const onClose = () => {\n    setIsLoading(false);\n    setFileUpload(null);\n    setShow(false);\n  };\n  const onPublish = async () => {\n    try {\n      setIsLoading(true);\n      const { data } = await publish({\n        variables: {\n          file: fileUpload.file,\n        },\n      });\n\n      if (!data.publish.status) {\n        toast.warning(\"error al publicar\");\n        isLoading(false);\n      } else {\n        onClose();\n      }\n    } catch (error) {}\n  };\n\n  return (\n    <Modal size=\"small\" open={show} onClose={onClose} className=\"modal-upload\">\n      <div\n        {...getRootProps()}\n        className=\"dropzone\"\n        style={fileUpload && { border: 0 }}\n      >\n        {!fileUpload && (\n          <>\n            <Icon name=\"cloud upload\" />\n            <p>Arrastra tu foto que quieras publicar</p>\n          </>\n        )}\n        <input {...getInputProps()} />\n      </div>\n      {fileUpload?.type === \"image\" && (\n        <div\n          className=\"image\"\n          style={{ backgroundImage: `url(\"${fileUpload.preview}\")` }}\n        />\n      )}\n\n      {fileUpload && (\n        <Button className=\"btn-upload btn-action\" onClick={onPublish}>\n          Publicar\n        </Button>\n      )}\n\n      {isLoading && (\n        <Dimmer active className=\"publishing\">\n          <Loader />\n          <p>Publicando...</p>\n        </Dimmer>\n      )}\n    </Modal>\n  );\n}\n","/home/charly/Documentos/proyectos/instaclone-client/src/components/Header/Search/Search.js",[],"/home/charly/Documentos/proyectos/instaclone-client/src/components/Modal/ModalUpload/index.js",[],"/home/charly/Documentos/proyectos/instaclone-client/src/gql/publication.js",[],"/home/charly/Documentos/proyectos/instaclone-client/src/components/Publications/index.js",[],"/home/charly/Documentos/proyectos/instaclone-client/src/components/Publications/Publications.js",[],"/home/charly/Documentos/proyectos/instaclone-client/src/components/Publications/PreviewPublication/PreviewPublication.js",[],"/home/charly/Documentos/proyectos/instaclone-client/src/components/Publications/PreviewPublication/index.js",[],"/home/charly/Documentos/proyectos/instaclone-client/src/components/Modal/ModalPublication/ModalPublication.js",[],"/home/charly/Documentos/proyectos/instaclone-client/src/components/Modal/ModalPublication/CommentForm/CommentForm.js",[],"/home/charly/Documentos/proyectos/instaclone-client/src/gql/comment.js",[],"/home/charly/Documentos/proyectos/instaclone-client/src/components/Modal/ModalPublication/Comments/Comments.js",[],"/home/charly/Documentos/proyectos/instaclone-client/src/components/Modal/ModalPublication/Comments/index.js",[],{"ruleId":"308","replacedBy":"309"},{"ruleId":"310","replacedBy":"311"},{"ruleId":"308","replacedBy":"312"},{"ruleId":"310","replacedBy":"313"},{"ruleId":"308","replacedBy":"314"},{"ruleId":"310","replacedBy":"315"},{"ruleId":"308","replacedBy":"316"},{"ruleId":"310","replacedBy":"317"},{"ruleId":"318","severity":1,"message":"319","line":4,"column":1,"nodeType":"320","endLine":4,"endColumn":46},{"ruleId":"308","replacedBy":"321"},{"ruleId":"310","replacedBy":"322"},{"ruleId":"308","replacedBy":"323"},{"ruleId":"310","replacedBy":"324"},{"ruleId"